  "scripts": {
    "clean": "rm -rf build || del /S build\\*",
    "cleanTest": "rm -rf test || del /S test\\*",
    "dev": "webpack-dev-server --open",
    "lint": "eslint_d --ignore-path .jshintignore --fix src",
    "prod": "npm run clean | webpack -p --display-optimization-bailout --env.prod=prod",
    "test": "npm run cleanTest | webpack  --progress --display-optimization-bailout --env.prod=test",
    "start": "npm run dev"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint_d --fix src",
      "git add"
    ]
  },
